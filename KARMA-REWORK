			pickingMode = false
		end
		if KEY == "l" then
			script["Mode"].Value = "Insane" 
			sick.SoundId = syn and getsynasset('Shardem_Insane.mp3') or getcustomasset('Shardem_Insane.mp3')
			sick.Volume = 2.5
			sick:Pause()
			pickingMode = false
		end
	end
end)

